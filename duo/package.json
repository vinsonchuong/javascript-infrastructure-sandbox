{
  "name": "duo-app",
  "private": "true",
  "devDependencies": {
    "duo": "^0.15.6",
    "duo-babel": "github:stephenmathieson/duo-babel#upgrade-to-babel-6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-runtime",
      "transform-decorators-legacy"
    ],
    "ignore": "components"
  },
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  }
}
